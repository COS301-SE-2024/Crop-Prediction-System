{
	"name": "nuxt-app",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "nuxt build",
		"dev": "nuxt dev",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"format": "prettier --write .",
		"lint": "eslint .",
		"lint.fix": "eslint . --fix",
		"test": "vitest --coverage.enabled=true"
	},
	"dependencies": {
		"@googlemaps/js-api-loader": "^1.16.6",
		"@nuxt/eslint": "^0.3.13",
		"@vuelidate/core": "^2.0.3",
		"@vuelidate/validators": "^2.0.4",
		"chart.js": "^4.4.3",
		"nuxt": "^3.11.2",
		"nuxt-gmaps": "^1.2.10",
		"nuxt-vitest": "^0.11.5",
		"primeicons": "^7.0.0",
		"primevue": "^3.52.0",
		"vue": "^3.4.27",
		"vue-router": "^4.3.2"
	},
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@nuxt/test-utils": "^3.13.1",
		"@nuxtjs/color-mode": "^3.4.1",
		"@nuxtjs/supabase": "^1.2.2",
		"@nuxtjs/tailwindcss": "^6.12.0",
		"@testing-library/vue": "^8.1.0",
		"@types/jest": "^29.5.12",
		"@vitejs/plugin-vue": "^5.0.5",
		"@vitest/coverage-v8": "^1.6.0",
		"@vue/test-utils": "^2.4.6",
		"autoprefixer": "^10.4.19",
		"happy-dom": "^14.12.0",
		"husky": "^9.0.11",
		"jsdom": "^24.1.0",
		"lint-staged": "^15.2.5",
		"nuxt-primevue": "^3.0.0",
		"playwright-core": "^1.44.1",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.5",
		"vitest": "^1.6.0"
	},
	"lint-staged": {
		"./**/*.vue": [
			"pnpm run format",
			"pnpm run lint"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "lint-staged"
		}
	}
